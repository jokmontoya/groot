{
    "name": "fostercommerce/groot",
    "description": "A static site generator built using Twig.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jaggy",
            "email": "jaggy@fostercommerce.com"
        }
    ],
    "require": {
        "twig/twig": "^2.5",
        "symfony/console": "^4.2",
        "illuminate/support": "^5.7",
        "illuminate/filesystem": "^5.7",
        "pimple/pimple": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "FosterCommerce\\Groot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "bin": [
        "groot"
    ]
}
